    "content_type_entries_title_496": {
        "uid": "content_type_entries_title_496",
        "title": "Creativity: 13 Ways to Look at It",
        "locale": "en-us",
        "created": "2019-09-09T13:09:29.000Z",
        "type": "article",
        "body": {
            "type": "doc",
            "uid": "55ffde6cd8844053b94d4f5475f7a72f",
